    "lucide-react": "^0.541.0",
    "next": "15.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^16.3.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^4",
    "tsx": "^4.7.0",
    "typescript": "^5"
